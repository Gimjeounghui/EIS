<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kun.eis.user.project.service.ProjectMapper">
    
    <!-- 프로젝트 리스트 쿼리 -->
    <select id="selectProjectList" resultType="projectVO" parameterType="projectVO">
    
    	SELECT 	  p_no
    			, p_title
    			, p_content
    			, p_created_ate
    			, p_updated_ate
    			, p_deleted_ate
    			, m_email
    	
    	FROM PROJECT		
    
    </select>
    
</mapper>